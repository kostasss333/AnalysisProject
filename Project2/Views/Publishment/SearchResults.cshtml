@model List<DAL.Entitites.Publishment>
@{
    ViewBag.Title = "SearchResults";
}


    <table class="table">
        <thead>
        <tr>
        </tr>
        </thead>
        <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <th>
                    <a href="@Url.Action("Index", "Publishment", new {id = item.ID})">@item.title by @item.authors</a>

                </th>
            </tr>
        }
        </tbody>
    </table>


